{
  "compilerOptions": {
      "outDir": "./dist/",
      "module": "commonjs",
      "preserveConstEnums": true,
      "noImplicitAny": true,
      "noImplicitThis": true,
      "strictNullChecks": true,
      "strictFunctionTypes": true,
      "sourceMap": true,
      "target": "es5",
      "allowJs": true
  },
  "exclude": [
      "node_modules",
      "./dist/**/*.js"
  ]
}